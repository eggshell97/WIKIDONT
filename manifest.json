{
  "manifest_version": 3,
  "name": "WIKIDONT",
  "version": "1.0",
  "description": "Bypasses Wikidot issues",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "WIKIDONT"
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikidot.com/*"],
      "js": ["content.js"]
    }
  ]
}
